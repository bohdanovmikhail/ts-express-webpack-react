{
  "name": "ts-express-webpack-react",
  "version": "0.0.1",
  "description": "Boilerplate for React app based on Typescript and builded with Webpack",
  "scripts": {
    "build:dev": "ts-node ./index.ts -e dev",
    "build:prod": "ts-node ./index.ts -e prod --compressOutput",
    "start:dev": "ts-node ./index.ts -e dev --watch --hot",
    "start": "npm run start:dev",
    "test": "ts-node ./test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohdanovmikhail/ts-express-webpack-react.git"
  },
